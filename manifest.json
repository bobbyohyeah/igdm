{
  "manifest_version": 3,
  "name": "Instagram but DM Only",
  "version": "1.01",
  "description": "A Chrome extension that enables IG DMs only.",
  "permissions": [],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["content.js"]
    }
  ]
}